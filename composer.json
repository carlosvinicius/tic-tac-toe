{
    "name": "carlosvinicius/tic-tac-toe",
    "description": "Tic Tac Toe Game",
    "keywords": ["tic-tac-toe", "php", "lumen", "game"],
    "authors": [
        {
            "name": "Carlos Vinicius Pereira da Silva",
            "email": "carlos@cvps.com.br",
            "homepage": "https://github.com/carlosvinicius/tic-tac-toe",
            "role": "Full Stack Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
